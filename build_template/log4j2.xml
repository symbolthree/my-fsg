<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" strict="true">
	<Appenders>
	  <Appender type="File" name="File" fileName="${sys:LOG_DIR}/myFSG.log">
	    <Layout type="PatternLayout">
	      <Pattern>%5p | %d{ISO8601} | %F | %L | %m%n</Pattern>
	    </Layout>
	  </Appender>
  </Appenders>
  <Loggers>
    <Root level="info">
     <AppenderRef ref="File"/>
    </Root>
  </Loggers>
</Configuration>